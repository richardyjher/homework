#include <stdio.h>
#include <stdlib.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char *argv[]) {
	float high,weight,BMI;
	printf("請輸入您的身高:");
	scanf("%f",&high);
	printf("請輸入您的體重:");
	scanf("%f",&weight);
	float x = high*high*0.0001;

	printf("您的BMI是:%f\n",weight/x);
	
    system("pause");
	return 0;
}
